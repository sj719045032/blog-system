<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <link rel="stylesheet" href="/js/bootstrap3/css/bootstrap.min.css"/>
    <script src="/js/jquery-2.1.4.min.js"></script>
    <script src="/js/bootstrap3/js/bootstrap.min.js"></script>
</head>
<body style="background: #73CFF1 url('/images/index_bg.jpg' ) center top no-repeat; ">
<% include top %>
<div class="container">
    <% include prompt.ejs%>
    <div class="row">
        <% if(filelist) %>
        <% filelist.forEach(function (file, index) { %>
        <div class="col-md-12 col-sm-12 table-bordered">

            <a class="h2 label label-default" href="/filemanage/d/<%= file.storeName %>"><%= file.originName %></a>

            <p class="info">
               <span class="label label-info">作者：<a
                           href="/users/<%= file.upLoadUserName %>"><%= file.upLoadUserName %></a>  |
                日期：<%= file.uploadTime %>
               </span>

        </div>
        <% }) %>
    </div>

    <% include pageing %>
    <% include bottom %>
</div>
</body>
</html>



