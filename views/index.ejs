<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
</head>
<body>
<div id="main">
    <% include top %>
    <div id="content">
        <% if (success) { %>
        <div><%= success %></div>
        <% } %>
        <% if (error) { %>
        <div><%= error %> </div>
        <% } %>
        <% posts.forEach(function (post, index) { %>
        <p>

        <h2><a href="#"><%= post.title %></a></h2></p>
        <p class="info">
            作者：<a href="#"><%= post.name %></a> |
            日期：<%= post.time.minute %>
        </p>

        <p><%- post.post %></p>
        <% }) %>
    </div>

    <% include bottom %>
</div>
</body>
</html>



