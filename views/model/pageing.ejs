<div  class="pager" >

    <% if (!isFirstPage) { %>
    <li class="prepage"><a title="上一页" href="?p=<%= page - 1 %>">上一页</a></li>
    <% } %>

    <% if (!isLastPage) { %>
    <li class="nextpage"><a title="下一页" href="?p=<%= page + 1 %>">下一页</a></li>
    <% } %>
    <li class="pagenow"><a><input size="1" class="page_input" ng-model="page" ng-init="page=<%= page %>">/<%= total %>页</a></li>
    <li class="pagego"><a title="跳转" href="?p={{page}}">跳转</a></li>
</div>
